nfo.goods_brief }}</p>\r\n      <p class=\"price\">￥{{ info.retail_price }}</p>\r\n      <div class=\"eject\">\r\n        <van-cell is-link @click=\"showPopup\" class=\"choose\"> 请选择规格数量 </van-cell>\r\n        <van-popup v-model=\"show\" position=\"bottom\" :style=\"{ height: '40%' }\" class=\"content\">\r\n          <!-- 弹出层内容 -->\r\n          <div class=\"top\">\r\n            <div class=\"left\">\r\n              <img :src=\"info.primary_pic_url\" />\r\n              <div class=\"leftTitle\">\r\n                <p class=\"price\">价格￥{{ info.retail_price }}</p>\r\n                <p class=\"num\">请选择数量</p>\r\n              </div>\r\n            </div>\r\n            <div class=\"right\"><van-icon name=\"cross\" /></div>\r\n          </div>\r\n          <div class=\"count\">\r\n            <p>数量</p>\r\n            <van-stepper v-model=\"value\" integer />\r\n          </div>\r\n          <div class=\"footer\">\r\n            <div><van-icon name=\"star-o\" /></div>\r\n            <div><van-icon name=\"shopping-cart-o\" /></div>\r\n            <div>立即购买</div>\r\n            <div>加入购物车</div>\r\n          </div>\r\n        </van-popup>\r\n      </div>\r\n    </div>\r\n    <div v-html=\"info.goods_desc\" class=\"desc\"></div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { goodsDetail } from '@/api/goods/index.js'\r\nexport default {\r\n  data() {\r\n    return {\r\n      gallery: [],\r\n      info: {},\r\n      show: false,\r\n      value: 1\r\n    }\r\n  },\r\n  methods: {\r\n    getList() {\r\n      const id = this.$store.state.goodsId\r\n      const openId = localStorage.getItem('openId')\r\n      goodsDetail({ id: id, openId: openId }).then(res => {\r\n        console.log(res)\r\n        this.gallery = res.gallery\r\n        this.info = res.info\r\n      })\r\n    },\r\n    onClickLeft() {\r\n      this.$router.push('/category/categories')\r\n    },\r\n    showPopup() {\r\n      this.show = true\r\n    },\r\n    // 点击立即购买\r\n    onBuyClicked() {},\r\n    // 点击添加到购物车\r\n    onAddCartClicked() {}\r\n  },\r\n  created() {\r\n    this.getList()\r\n  }\r\n}\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n.container {\r\n  background-color: #eee;\r\n  .my-swipe {\r\n    .swiperItem {\r\n      img {\r\n        width: 100%;\r\n      }\r\n    }\r\n  }\r\n  .title {\r\n    display: flex;\r\n    justify-content: space-around;\r\n    p {\r\n      color: #666;\r\n      font-size: 12px;\r\n      padding: 10px 0;\r\n    }\r\n  }\r\n  .info {\r\n    background-color: #fff;\r\n    text-align: center;\r\n    margin-bottom: -8px;\r\n    .name {\r\n      font-size: 20px;\r\n      font-weight: 500;\r\n      padding: 10px;\r\n    }\r\n    .title {\r\n      color: #999;\r\n      font-size: 12px;\r\n      margin: 12px 0;\r\n    }\r\n    .price {\r\n      color: #b4282d;\r\n      font-size: 17px;\r\n      margin: 5px 0 0 0;\r\n      padding-bottom: 30px;\r\n      border-bottom: 1px solid #eee;\r\n    }\r\n    .eject {\r\n      .choose {\r\n        font-size: 12px;\r\n        padding: 10px 10px 10px 10px;\r\n      }\r\n      .content {\r\n        .top {\r\n          display: flex;\r\n          justify-content: space-between;\r\n          padding: 15px;\r\n          .left {\r\n            display: flex;\r\n            img {\r\n              width: 88px;\r\n              height: 88px;\r\n              margin-right: 15px;\r\n            }\r\n            .leftTitle {\r\n              display: flex;\r\n              flex-direction: column;\r\n              align-items: center;\r\n              justify-content: center;\r\n\r\n              .price {\r\n                color: #b4282d;\r\n                font-size: 12px;\r\n                padding: 10px 0;\r\n                border: none;\r\n              }\r\n              .num {\r\n                font-size: 12px;\r\n                margin: 5px 0;\r\n              }\r\n            }\r\n          }\r\n        }\r\n        .count {\r\n          text-align: left;\r\n          margin-left: 15px;\r\n        }\r\n        .footer {\r\n          display: flex;\r\n          justify-content: space-around;\r\n          align-items: center;\r\n          margin-top: 30px;\r\n          position: fixed;\r\n          bottom: 0;\r\n          left: 0;\r\n        }\r\n      }\r\n    }\r\n  }\r\n  .desc {\r\n    ::v-deep img {\r\n      width: 100%;\r\n    }\r\n  }\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}