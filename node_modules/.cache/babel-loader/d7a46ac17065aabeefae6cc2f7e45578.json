{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { getOrderInfo } from '@/api/order/index.js';\nexport default {\n  data() {\n    return {\n      address: {},\n      total: '',\n      list: []\n    };\n  },\n  methods: {\n    // 点击返回\n    onClickLeft() {\n      this.$router.go(-1);\n    },\n    // 点击进入地址详情页\n    goAddress() {\n      this.$router.push('/address');\n    },\n    // 初始化页面的信息\n    getInfo() {\n      const id = this.$route.params.id;\n      getOrderInfo({\n        openId: localStorage.getItem('openId'),\n        addressId: id || 11445\n      }).then(res => {\n        this.address = res.address;\n        this.total = res.allPrise;\n      });\n    }\n  },\n  created() {\n    this.getInfo();\n    this.list = this.$route.params.arr;\n  }\n};","map":{"version":3,"mappings":";AAqDA;AACA;EACAA;IACA;MACAC;MACAC;MACAC;IACA;EACA;EACAC;IACA;IACAC;MACA;IACA;IACA;IACAC;MACA;IACA;IACA;IACAC;MACA;MACAC;QAAAC;QAAAC;MAAA;QACA;QACA;MACA;IACA;EACA;EACAC;IACA;IACA;EACA;AACA","names":["data","address","total","list","methods","onClickLeft","goAddress","getInfo","getOrderInfo","openId","addressId","created"],"sourceRoot":"src/views/order","sources":["index.vue"],"sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <!-- 导航栏 -->\r\n    <van-sticky>\r\n      <van-nav-bar title=\"乐购\" left-text=\"返回\" left-arrow @click-left=\"onClickLeft\" />\r\n    </van-sticky>\r\n    <div class=\"address\" @click=\"goAddress\">\r\n      <div class=\"left\">\r\n        <div class=\"top\">\r\n          <p class=\"name\">{{ address.name }}</p>\r\n          <p class=\"default\" v-if=\"address.is_default\">默认</p>\r\n        </div>\r\n        <div class=\"footer1\">\r\n          <p class=\"phone\">{{ address.mobile }}</p>\r\n          <p class=\"address1\">{{ address.address + address.address_detail }}</p>\r\n        </div>\r\n      </div>\r\n      <div class=\"right\"><van-icon name=\"arrow\" size=\"20\" /></div>\r\n    </div>\r\n    <div class=\"goodsInfo\">\r\n      <div class=\"item\">\r\n        <p class=\"total\">商品合计</p>\r\n        <p class=\"price\">￥{{ total }}</p>\r\n      </div>\r\n      <div class=\"item\">\r\n        <p class=\"total\">运费</p>\r\n        <p class=\"price\">免运费</p>\r\n      </div>\r\n      <div class=\"item\">\r\n        <p class=\"total\">优惠券</p>\r\n        <p class=\"price\">暂无</p>\r\n      </div>\r\n    </div>\r\n    <div class=\"item\">\r\n      <div class=\"goods\" v-for=\"item in list\" :key=\"item.id\">\r\n        <div class=\"left\">\r\n          <img :src=\"item.list_pic_url\" />\r\n          <div class=\"info\">\r\n            <p>{{ item.goods_name }}</p>\r\n            <p>{{ item.retail_price }}</p>\r\n          </div>\r\n        </div>\r\n        <div class=\"count\">X{{ item.number }}</div>\r\n      </div>\r\n    </div>\r\n    <div class=\"footer\">\r\n      <div class=\"money\">实付：￥{{ total }}</div>\r\n      <div class=\"pay\">支付</div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { getOrderInfo } from '@/api/order/index.js'\r\nexport default {\r\n  data() {\r\n    return {\r\n      address: {},\r\n      total: '',\r\n      list: []\r\n    }\r\n  },\r\n  methods: {\r\n    // 点击返回\r\n    onClickLeft() {\r\n      this.$router.go(-1)\r\n    },\r\n    // 点击进入地址详情页\r\n    goAddress() {\r\n      this.$router.push('/address')\r\n    },\r\n    // 初始化页面的信息\r\n    getInfo() {\r\n      const id = this.$route.params.id\r\n      getOrderInfo({ openId: localStorage.getItem('openId'), addressId: id || 11445 }).then(res => {\r\n        this.address = res.address\r\n        this.total = res.allPrise\r\n      })\r\n    }\r\n  },\r\n  created() {\r\n    this.getInfo()\r\n    this.list = this.$route.params.arr\r\n  }\r\n}\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n.container {\r\n  width: 100%;\r\n  overflow: hidden;\r\n  background-color: #eee;\r\n  .address {\r\n    background: url('@/assets//images/address-bg-bd.png') 0 0 repeat-x #fff;\r\n    margin: 0 0 10px;\r\n    padding: 25px 15px 15px;\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: space-between;\r\n    .left {\r\n      display: flex;\r\n      align-items: center;\r\n      .top {\r\n        display: flex;\r\n        flex-direction: column;\r\n        margin: 10px 20px 10px 5px;\r\n        .name {\r\n          margin-bottom: 10px;\r\n        }\r\n        .default {\r\n          border: 1px solid #b4282d;\r\n          color: #b4282d;\r\n\r\n          padding: 0 2px;\r\n          text-align: center;\r\n        }\r\n      }\r\n      .footer1 {\r\n        display: flex;\r\n        flex-direction: column;\r\n        width: 240px;\r\n        .phone {\r\n          margin-bottom: 10px;\r\n        }\r\n        .address1 {\r\n          width: 260px;\r\n          white-space: nowrap;\r\n          text-overflow: ellipsis;\r\n          overflow: hidden;\r\n        }\r\n      }\r\n    }\r\n  }\r\n  .goodsInfo {\r\n    background-color: #fff;\r\n    margin-top: 10px;\r\n    padding: 15px 15px 10px 20px;\r\n    .item {\r\n      display: flex;\r\n      justify-content: space-between;\r\n      border-bottom: 2px solid #eee;\r\n      padding: 10px 0;\r\n    }\r\n  }\r\n  .item {\r\n    margin-top: 10px;\r\n    background-color: #fff;\r\n    .goods {\r\n      display: flex;\r\n      justify-content: space-between;\r\n      .left {\r\n        width: 100%;\r\n        display: flex;\r\n        img {\r\n          width: 62px;\r\n          height: 62px;\r\n          background-color: @=#eee;\r\n        }\r\n      }\r\n    }\r\n  }\r\n  .footer {\r\n    width: 100%;\r\n    height: 50px;\r\n    position: fixed;\r\n    bottom: 0;\r\n    left: 0;\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: space-between;\r\n    .money {\r\n      padding-left: 12px;\r\n    }\r\n    .pay {\r\n      background-color: #b4282d;\r\n      color: #fff;\r\n      width: 100px;\r\n      text-align: center;\r\n      height: 50px;\r\n      line-height: 50px;\r\n      font-size: 14px;\r\n    }\r\n  }\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}